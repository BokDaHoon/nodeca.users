.modal.fade(tabindex='-1' role='dialog' aria-hidden='true')
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        button.close(data-dismiss='modal')
          span(aria-hidden='true') &times;
          span.sr-only= self.t('close')
        h4.modal-title= self.t('title')
      .modal-body
