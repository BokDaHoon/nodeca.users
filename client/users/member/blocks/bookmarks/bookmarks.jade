.member-block

  .member-block__header
    .member-block__header-title
      a.member-block__header-link.pull-xs-left(href='#')
        = self.t('title')
        span.member-block__header-arrow.icon.icon-right-open
        small.member-block__header-counter 5
      a.member-block__header-collapser(href='#member-bookmarks-info', data-toggle='collapse')
        .pull-xs-right.icon.icon-collapse
        .pull-xs-right.icon.icon-expand

  #member-bookmarks-info.member-block__inner.collapse.in
    ul.member-block__bookmarks-info
      li.member-block__bookmarks-info-item
        a(href='#') Inventore provident fugiat minima
      li.member-block__bookmarks-info-item
        a(href='#') Accusamus velit nulla
      li.member-block__bookmarks-info-item
        a(href='#') Odit consectetur voluptates
